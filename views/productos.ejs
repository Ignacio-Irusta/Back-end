
<h2 style="color:crimson;">Producto</h2>
<% const articulosCargados = producto.length > 0; %>
<% if(articulosCargados){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: blueviolet;">
                <th>Titulo</th>
                <th>Precio</th>
                <th>Imagen</th>
            </tr>
            <% for (const articulos of producto) {%>
                <tr>
                    <td>
                        <%=articulos.title%>
                    </td>
                    <td>
                        <%=articulos.price%>
                    </td>
                    <td>
                        <%=articulos.tumbline%>
                    </td>
                </tr>
                <%};%>
        </table>
    </div>
        <% } else { %>
            <h3 class="alert alert-danger">no se encontraron datos</h3>
            <% } %>